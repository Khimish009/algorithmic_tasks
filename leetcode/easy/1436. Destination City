Solution 1:
    Time complexity O(n)
    Space complexity O(n)

var destCity = function(paths) {
    const hashMap = new Map()

    for (const [cityA, cityB] of paths) {
        hashMap.set(cityA, cityB)
    }

    for (const [_, cityB] of paths) {
        if (!hashMap.has(cityB)) {
            return cityB
        }
    }

    return ""
};
