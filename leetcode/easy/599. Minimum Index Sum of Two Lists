Solution:
    Time complexity O(n)
    Space complexity O(n)

var findRestaurant = function(list1, list2) {
    const hashMap1 = new Map()
    const hashMap2 = new Map()
    let min = Number.MAX_SAFE_INTEGER

    for (let i = 0; i < list1.length; i++) {
        hashMap1.set(list1[i], i)
    }

    for (let i = 0; i < list2.length; i++) {
        if (hashMap1.has(list2[i])) {
            const sum = i + hashMap1.get(list2[i])
            hashMap2.set(list2[i], sum)

            if (sum < min) {
                min = sum
            }
        }
    }

    return [...hashMap2.entries()].filter(([_, value]) => value === min).map(([key]) => key)
};
